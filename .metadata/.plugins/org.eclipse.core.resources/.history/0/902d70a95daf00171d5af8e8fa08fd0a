package groupTheory;

public class CompositionMapping {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int x=1;x<11;x++){
			for(int y=1;y<11;y++){
				if(x==1&&y==1){
					System.out.println(x*y+"	");
					//x++;y++;
				}
				System.out.print(x*y+"	");
			}
			System.out.println();
		}

	}

}
